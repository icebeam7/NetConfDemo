<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NetConfDemo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:NetConfDemo.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Photo"
        ContentTemplate="{DataTemplate views:PhotoPage}"
        Route="PhotoPage" />

    <ShellContent
        Title="Draw"
        ContentTemplate="{DataTemplate views:DrawPage}"
        Route="DrawPage" />

    <ShellContent
        Title="Photo Service"
        ContentTemplate="{DataTemplate views:PhotoServicePage}"
        Route="PhotoServicePage" />

</Shell>
